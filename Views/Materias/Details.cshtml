@model ColegioSanJose.Models.Materia

@{
    ViewData["Title"] = "Detalles de la Materia";
}

<h1>Detalles de la Materia</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NombreMateria)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NombreMateria)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Docente)
        </dt>
